Lock 229 Doom
{
	Any { BlueCard BlueSkull KeyBlue}
	Any { YellowCard YellowSkull KeyYellow}
	Any { RedCard RedSkull KeyGreen}
	Message "$PD_ALL3"
	RemoteMessage "$PD_ALL3O"
}

