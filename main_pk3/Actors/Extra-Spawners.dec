Actor ExtraResetFlagDROPPED : CustomInventory
{
-COUNTITEM
+NOTIMEFREEZE
+INVENTORY.QUIET
-INVENTORY.INVBAR
+INVENTORY.ALWAYSPICKUP
+INVENTORY.AUTOACTIVATE
Inventory.Amount 1
Inventory.MaxAmount 0
States
	{
	Pickup:
		TNT1 A 0
		TNT1 A 0 A_ChangeFlag(DROPPED,0)
		TNT1 A 0 A_ChangeFlag("INVENTORY.NEVERRESPAWN",0)
		Stop
	}
}

Actor ExtraSentryBoxSpawner 32765
{
//$Category Ammunition
//$Sprite SENBA0
//$Title "Sentry Gun Box"
+RANDOMIZE
+NOINTERACTION
+MOVEWITHSECTOR
var int user_tid;
States
	{
	Spawn:
		TNT1 A 4
		TNT1 A 1 ACS_NamedExecuteAlways("Extra_SelectItemSpawn",0))
		TNT1 A 1
		Wait
	External:
		TNT1 A 1 A_SpawnItemEx("SentryBox",0,0,8,0,0,0,0,SXF_NOCHECKPOSITION,0,ACS_NamedExecuteWithResult("Extra_GetTID"))
		TNT1 A 10 ACS_NamedExecuteWithResult("Extra_SetupItem"))
		Stop
	Include:
		TNT1 A 1 A_SpawnItemEx("NMSentryBox",0,0,8,0,0,0,0,SXF_NOCHECKPOSITION,0,ACS_NamedExecuteWithResult("Extra_GetTID"))
		TNT1 A 10 ACS_NamedExecuteWithResult("Extra_SetupItem"))
		Stop
	}
}
